		dark_gray = White_OFF;
		White = White_OFF;
		Border_L = White_OFF;
		Border_D = White_OFF;
	}
	GPIOC->ODR = White_OFF;
	GPIOC->ODR = dark_gray;
	while (waste_of_time<72)
	{
