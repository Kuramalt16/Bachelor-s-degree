	}
	}
	}

	void test_colours(int starting_colour)
	{
	int not_colour = !starting_colour;
	int colour = 0b10;
	int i = 0;
	colour = starting_colour;
	while(i < 10)
	{
	GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			GPIOC->ODR |=colour;
			not_colour = !colour;
			GPIOC->ODR &=not_colour;
			GPIOC->ODR &=not_colour;
			GPIOC->ODR &=not_colour;
			GPIOC->ODR &=not_colour;
			colour = colour+0b10;
			i++;
	}
	i=0;
	}

/* USER CODE END 1 */
