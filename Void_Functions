		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
		not_colour = !colour;
		GPIOC->ODR &=not_colour;
		colour = colour+0b10;
		i++;
		}
		i=0;
		break;
		case 140 ... 149:
		colour = 0x1F4C;
		while (i<10)
		{
		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
		GPIOC->ODR |=colour;
