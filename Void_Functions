	GPIOC->ODR &= White_OFF;
	GPIOC->ODR &= White_OFF;

	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;

	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;

	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;
	GPIOC->ODR |= RED_1_ON;
