	void colours(int page)
	{
	int colour = 0b10;
	int i = 0;
	if (page == 0)
	{
	switch(Lines)
	{
	int not_colour;
	case 0 ... 9:
	colour = 0x2;
	while (i<10)
	{
	GPIOC->ODR |=colour;
	GPIOC->ODR |=colour;
	GPIOC->ODR |=colour;
	GPIOC->ODR |=colour;
	GPIOC->ODR |=colour;
	GPIOC->ODR |=colour;
	GPIOC->ODR |=colour;
	not_colour = !colour;
	GPIOC->ODR &=not_colour;
	colour = colour+0b10;
	i++;
	}
	i=0;
	break;

	case 10 ... 19:
	colour = 0x16;
	while (i<10)
	{
	GPIOC->ODR |=colour;
