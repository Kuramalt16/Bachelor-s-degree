	{
	GPIOC->ODR = White;
	waste_of_time++;
	}
	waste_of_time=0;
	GPIOC->ODR = dark_gray;
	GPIOC->ODR = White_OFF;
	GPIOC->ODR = light_gray;
	GPIOC->ODR = light_gray;
	GPIOC->ODR = White;
	GPIOC->ODR = White_OFF;
