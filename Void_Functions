	GPIOC->ODR |= RED_1_ON;

	GPIOC->ODR &= White_OFF;
	GPIOC->ODR &= White_OFF;
	GPIOC->ODR &= White_OFF;
	GPIOC->ODR &= White_OFF;

	break;
	case 91 ... 150://ccase 3

	while(waste_of_time < 5)
