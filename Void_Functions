		GPIOC->ODR = Deep_Green;

		GPIOC->ODR = Score_val[222];
		GPIOC->ODR = Score_val[222];

		GPIOC->ODR = Score_val[223];
		GPIOC->ODR = Score_val[223];

		GPIOC->ODR = Score_val[224];
		GPIOC->ODR = Score_val[224];

